.flex-wrp{
  height: 300rpx;
  background: #fff; 
  padding: 16rpx 30rpx 0 30rpx; 
} 

.item-box {
  width:100%;
  display: flex; 
  flex-direction: row; 
  flex-wrap: wrap; 
  justify-content: space-between; 
}

.flex-item{
  width: 100rpx;
  text-align: center;
  padding: 0 10rpx;
  margin-bottom: 20rpx;
}

.icon{
  width: 80rpx;
}
.icon-img{
  width: 100%;
  height: 40px;
  margin-left: 9rpx;
}
.text{
  font-size: 24rpx;
}